{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "kitty",
        "source": "${HOME}/.config/fastfetch/arch.png",
        "height": 17,
        "width": 34,
        "padding": {
            "top": 2,
            "left": 2,
            "right": 2
        }
        //"color": {
        //    "1": "blue",
        //}
    },
    "modules": [
        "break",
        {
            "type": "custom",
            "format": "\u001b[38;5;245mHardware"
        },
        {
            "type": "host",
            "key": " PC",
            "keyColor": "#7d9bba"
        },
        {
