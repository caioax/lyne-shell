            "keyColor": "#7d9bba"
        },
        {
            "type": "cpu",
            "key": "└ └󰍛",
            "keyColor": "#7d9bba"
        },
        // {
        //     "type": "memory",
        //     "key": "│ ├󰍛",
        //     "keyColor": "#7d9bba"
        // },
        // {
        //     "type": "disk",
        //     "key": "└ └",
        //     "keyColor": "#7d9bba"
        // },
        "break",
        {
            "type": "custom",
            "format": "\u001b[38;5;245mSoftware"
        },
        {
            "type": "os",
            "key": " OS",
            "keyColor": "#7d9bba"
        },
        {
            "type": "kernel",
            "key": "│ ├",
            "keyColor": "#7d9bba"
        },
        // {
        //     "type": "bios",
        //     "key": "│ ├",
        //     "keyColor": "#7d9bba"
        // },
        {
            "type": "packages",
            "key": "│ ├󰏖",
            "keyColor": "#7d9bba"
        },
        {
            "type": "shell",
            "key": "│ ├",
            "keyColor": "#7d9bba"
        },
        // "break",
        {
            "type": "de",
            "key": " DE",
            "keyColor": "#7d9bba"
        },
        {
            "type": "lm",
            "key": "│ ├",
            "keyColor": "#7d9bba"
        },
        {
            "type": "wm",
            "key": "│ ├",
            "keyColor": "#7d9bba"
        },
        {
            "type": "wmtheme",
            "key": "│ ├󰉼",
            "keyColor": "#7d9bba"
        },
        {
            "type": "terminal",
            "key": "└ └",
            "keyColor": "#7d9bba"
        },
        "break",
        {
            "type": "custom",
            "format": "\u001b[38;5;245mAge / Uptime"
        },
        {
            "type": "command",
            "key": " OS Age",
            "keyColor": "#7d9bba",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
        },
        {
            "type": "uptime",
            "key": " Uptime",
            "keyColor": "#7d9bba"
        },
        // {
        //     "type": "datetime",
        //     "key": " Date Time",
        //     "keyColor": "#7d9bba"
        // },
        "break",
        {
            "type": "colors",
            "paddingLeft": 0,
            "symbol": "circle"
        }
 
    ]
